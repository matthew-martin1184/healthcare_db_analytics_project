# Oracle Autonomous Database Credentials
DB_USER=your_username
DB_PASSWORD=your_password
DB_DSN=your_db_high_or_medium_string   # e.g. 'adb.us-ashburn-1.oraclecloud.com/your_tns_alias'

# Optional: Path to Oracle wallet (if using it locally)
DB_WALLET_PATH=/path/to/your_wallet

# Streamlit Config (Optional - helpful for deployment or toggling settings)
STREAMLIT_SECRET_KEY=your_secret_key_here
